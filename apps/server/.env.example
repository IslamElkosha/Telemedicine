# Environment
NODE_ENV=development
PORT=3001

# Database
DATABASE_URL=postgresql://postgres:password@localhost:5432/telemedicine

# Redis
REDIS_URL=redis://localhost:6379

# JWT
JWT_SECRET=your-super-secret-jwt-key-at-least-32-characters-long
JWT_EXPIRES_IN=15m
JWT_REFRESH_EXPIRES_IN=7d

# CORS
APP_ORIGIN=https://comprehensive-teleme-pbkl.bolt.host

# Storage (S3-compatible)
S3_ENDPOINT=http://localhost:9000
S3_BUCKET=telemedicine
S3_ACCESS_KEY=minioadmin
S3_SECRET_KEY=minioadmin

# Email
EMAIL_PROVIDER=RESEND
RESEND_API_KEY=re_your_resend_api_key

# SMS/WhatsApp
SMS_PROVIDER=INFOBIP
INFOBIP_API_KEY=your_infobip_api_key

# Payments
PAYMENT_PROVIDER=STRIPE
STRIPE_SECRET=sk_test_your_stripe_secret_key
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret
ACCEPT_API_KEY=your_accept_api_key
ACCEPT_IFRAME_ID=your_accept_iframe_id
ACCEPT_HMAC=your_accept_hmac_secret